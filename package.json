{
  "name": "healthapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.js",
    "precommit": "lint-staged",
    "pods": "cd ios && pod install",
    "pods-m1": "cd ios && arch -x86_64 pod install",
    "gradlew": "cd android && ./gradlew clean && ./gradlew installDebug",
    "gradlew-m1": "cd android && ./gradlew clean && cd .. && npm cache clean --force",
    "kill-port": "npx kill-port 8081",
    "dev": "nodemon server-stripe.js"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --ext .js",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@firebase/auth": "^0.20.5",
    "@react-native-async-storage/async-storage": "^1.17.9",
    "@react-native-community/netinfo": "^9.3.0",
